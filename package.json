        "restify-jwt-community": "^1.0.9",
        "rimraf": "^2.6.3",
        "ts-node": "^8.0.1",
        "tslint": "^5.12.1",
        "typeorm": "^0.2.12",
        "typescript": "^3.3.1",
        "winston": "^3.2.1",
        "yargs": "^12.0.5"
