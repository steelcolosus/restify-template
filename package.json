    },
    "homepage": "https://github.com/steelcolosus/restify-template#readme",
    "dependencies": {
        "@types/bcrypt": "^3.0.0",
        "@types/dotenv": "^6.1.0",
        "@types/faker": "^4.1.5",
        "@types/http-status": "^0.2.30",
        "@types/jsonwebtoken": "^8.3.0",
        "@types/restify": "^7.2.7",
        "@types/restify-cors-middleware": "^1.0.1",
        "@types/restify-errors": "^4.3.3",
        "@types/yargs": "^12.0.8",
        "bcrypt": "^3.0.3",
