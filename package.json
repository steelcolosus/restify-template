{
    "name": "restify.template",
    "version": "1.0.0",
    "description": "template for REST api projects",
    "main": "index.js",
    "scripts": {
        "start": "nodemon --exec ts-node src/index.ts",
        "fixtures": "ts-node src/fixtures/*.ts",
        "migration:generate": "npx ts-node src/migrations/index.ts migration:generate $*",
        "migration:run": "npx ts-node src/migrations/index.ts migration:run $*"
    },
